    "prepare": "yarn run build"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.1",
    "express": "^4.15.4",
    "mocha": "^4.0.1",
    "mocha-clean": "^1.0.0",
    "rxjs": "^5.5.10",
    "rxjs-marbles": "^2.3.3",
    "webpack": "^3.8.1",
    "webpack-dev-middleware": "^1.12.0",
    "ws": "^3.2.0"
  },
  "peerDependencies": {
    "rxjs": "^5.0.0"
  }
}
