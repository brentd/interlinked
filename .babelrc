{
  "presets": [
    ["env", {"targets": {"node": "current"}}]
  ],
  "plugins": ["babel-plugin-transform-class-properties"],
  "env": {
    "commonjs": {
      "presets": [["env", {"targets": {"browsers": ["ie >= 11"]}}]],
      "plugins": ["babel-plugin-transform-class-properties"],
      "ignore": ["**.test.js"]
